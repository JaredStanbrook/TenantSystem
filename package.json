{
  "name": "tenantsystem",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bun run typecheck && bun run build:client && bun run build:server",
    "build:client": "bunx --bun vite build --mode client",
    "build:server": "bunx --bun vite build",
    "typecheck": "tsc -b",
    "gen": "bunx drizzle-kit generate && bunx wrangler types",
    "migrate:local": "bun run gen && bunx wrangler d1 migrations apply sheoak_ct",
    "migrate:remote": "bun run gen && bunx wrangler d1 migrations apply sheoak_ct --remote",
    "deploy:staging": "bun run migrate:remote && bun run build && bunx wrangler deploy --env staging",
    "deploy:prod": "bun run migrate:remote && bun run build && bunx wrangler deploy",
    "lint": "eslint .",
    "preview": "bun run build && bunx wrangler dev",
    "prepare": "husky",
    "test": "vitest"
  },
  "dependencies": {
    "@hono-rate-limiter/cloudflare": "^0.2.2",
    "@hono/vite-dev-server": "^0.23.0",
    "@hono/zod-validator": "^0.5.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2",
    "@simplewebauthn/types": "^12.0.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.9.0",
    "lit": "^3.3.1",
    "lucia": "^3.2.2",
    "lucide": "^0.560.0",
    "lucide-react": "^0.557.0",
    "next-themes": "^0.4.6",
    "otpauth": "^9.4.1",
    "qrcode": "^1.5.4",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind": "^4.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "wrangler": "^4.28.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@hono/vite-build": "^1.8.0",
    "@types/node": "^24.10.2",
    "@types/qrcode": "^1.5.6",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.2.7",
    "vitest": "^3.2.4"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "core-js",
    "esbuild",
    "sharp",
    "workerd"
  ]
}